{
  "manifest_version": 2,
  "name": "GitHub pull request branch quick select",
  "description": "Reorders branch drop-downs on a GitHub pull request by moving optional favourite branches to the top of the list.",
  "version": "1.0.1.0",
  "browser_action": {
    "default_icon": "content/assets/images/gprbqs128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "content/assets/images/gprbqs16.png",
    "48": "content/assets/images/gprbqs48.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "content/js/jquery-3.5.1.min.js",
      "content/js/core.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.github.com/*",
        "*://github.com/*"
      ],
      "js": [
        "content/js/jquery-3.5.1.min.js",
        "content/js/core.js",
        "content/js/init.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "*://www.github.com/*",
    "*://github.com/*"
  ]
}